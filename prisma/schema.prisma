generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model MockInterview {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  userId        String 
  MockResponse  Json[]
  jobPosition   String
  jobDesc       String
  jobexperience String
  createdAt     DateTime @default(now())
}
